// Prisma schema placeholder for GPT-5 generated UI/UX data models.
// This file will be expanded by Codex automation once specs are compiled.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UiuxSpec {
  id          String   @id @default(cuid())
  name        String
  createdAt   DateTime @default(now())
  description String?
}
